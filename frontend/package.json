    "react-images-uploading": "^3.1.3",
    "react-places-autocomplete": "^7.3.0",
    "react-responsive-carousel": "^3.2.23",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "react-select": "^5.2.2",
    "socket.io-client": "^4.7.2",
    "styled-components": "^5.3.5",
    "uuid": "^8.3.2",
