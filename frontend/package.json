    "@testing-library/user-event": "^12.8.3",
    "axios": "^0.21.4",
    "emoji-picker-react": "^3.5.1",
    "env-cmd": "^10.1.0",
    "js-cookie": "^2.2.1",
    "query-string": "^7.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-fontawesome": "^1.7.1",
    "react-google-location": "^1.2.2",
